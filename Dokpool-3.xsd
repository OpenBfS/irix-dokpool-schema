                </xs:documentation>
              </xs:annotation>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="DokpoolName" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:annotation>
                <xs:documentation>
                  <xs:enumeration value="bund"/>
                </xs:documentation>
              </xs:annotation>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="DokpoolGroupFolder" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:annotation>
                <xs:documentation>
                  <xs:enumeration value="bund_e-rb"/>
