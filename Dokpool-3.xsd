                  <xs:enumeration value="L51"/>
                  <xs:enumeration value="L52"/>
                  <xs:enumeration value="L53"/>
                </xs:documentation>
              </xs:annotation>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>

        <xs:element name="SampleType">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:annotation>
                <xs:documentation>
                  <xs:enumeration value="Niederschlag"/>
                  <xs:enumeration value="Niederschlag (Aktivitätskonzentration)"/>
                  <xs:enumeration value="nasse Niederschläge (Deposition)"/>
                  <xs:enumeration value="trockene Niederschläge (Deposition)"/>
                </xs:documentation>
              </xs:annotation>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>

