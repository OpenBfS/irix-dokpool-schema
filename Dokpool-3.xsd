        <xs:element default="true" name="IsDoksys" type="xs:boolean" minOccurs="0">
        </xs:element>

        <xs:element default="false" name="IsRodos" type="xs:boolean" minOccurs="0">
        </xs:element>

        <xs:element default="false" name="IsRei" type="xs:boolean" minOccurs="0">
        </xs:element>

        <xs:element name="ELAN">
          <xs:complexType>
            <xs:sequence>

              <xs:element name="ElanScenarios" minOccurs="0" maxOccurs="1">
