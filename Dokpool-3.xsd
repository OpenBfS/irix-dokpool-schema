            </xs:restriction>
          </xs:simpleType>
        </xs:element>

        <xs:element name="DataType" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:annotation>
                <xs:documentation>
                  <xs:enumeration value="LaDa"/>
                  <xs:enumeration value="OnMon"/>
                </xs:documentation>
              </xs:annotation>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>

