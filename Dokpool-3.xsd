              </xs:annotation>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>

        <xs:element name="DokpoolContentType">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:annotation>
                <xs:documentation>
