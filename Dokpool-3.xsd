            <xs:restriction base="xs:string">
            </xs:restriction>
          </xs:simpleType>
        </xs:element>

        <xs:element name="TrajectoryEndTime" type="xs:dateTime">
          <xs:simpleType>
            <xs:restriction base="xs:string">
            </xs:restriction>
          </xs:simpleType>
        </xs:element>

